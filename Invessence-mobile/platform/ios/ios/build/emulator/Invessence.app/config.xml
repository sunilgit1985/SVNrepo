<?xml version='1.0' encoding='utf-8'?>
<widget id="com.invessence" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Battery">
        <param name="ios-package" value="CDVBattery" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Compass">
        <param name="ios-package" value="CDVCompass" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="resource/iOS/Resources/icons/Icon-180@3x.png" width="180" />
    <icon height="120" src="resource/iOS/Resources/icons/Icon-120.png" width="120" />
    <icon height="76" src="resource/iOS/Resources/icons/Icon-76.png" width="76" />
    <icon height="152" src="resource/iOS/Resources/icons/Icon-152.png" width="152" />
    <icon height="40" src="resource/iOS/Resources/icons/Icon-40.png" width="40" />
    <icon height="80" src="resource/iOS/Resources/icons/Icon-80.png" width="80" />
    <icon height="57" src="resource/iOS/Resources/icons/Icon.png" width="57" />
    <icon height="114" src="resource/iOS/Resources/icons/Icon-114.png" width="114" />
    <icon height="72" src="resource/iOS/Resources/icons/Icon-72.png" width="72" />
    <icon height="144" src="resource/iOS/Resources/icons/Icon-72@2x.png" width="144" />
    <icon height="29" src="resource/iOS/Resources/icons/Icon-29.png" width="29" />
    <icon height="58" src="resource/iOS/Resources/icons/Icon-58.png" width="58" />
    <icon height="50" src="resource/iOS/Resources/icons/Icon-50.png" width="50" />
    <icon height="100" src="resource/iOS/Resources/icons/Icon-100.png" width="100" />
    <splash height="480" src="resource/iOS/Resources/splash/Default~iphone.png" width="320" />
    <splash height="960" src="resource/iOS/Resources/splash/Default@2x.png" width="640" />
    <splash height="1024" src="resource/iOS/Resources/splash/Default-Portrait.png" width="768" />
    <splash height="2048" src="resource/iOS/Resources/splash/Default-Portrait@2x.png" width="1536" />
    <splash height="768" src="resource/iOS/Resources/splash/Default-Landscape~ipad_1024x768.png" width="1024" />
    <splash height="1536" src="resource/iOS/Resources/splash/Default-Landscape@2x.png" width="2048" />
    <splash height="1136" src="resource/iOS/Resources/splash/Default-568h@2x.png" width="640" />
    <splash height="1334" src="resource/iOS/Resources/splash/Default-667h@2x.png" width="750" />
