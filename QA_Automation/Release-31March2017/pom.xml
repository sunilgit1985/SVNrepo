   <build>
     <plugins>

	<!-- Includes the runtime dependencies -->
 <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.18.1</version>
        <configuration>
          <suiteXmlFiles>
            <suiteXmlFile>TestNGXmls/SanityPreUpgrade.xml</suiteXmlFile>
          </suiteXmlFiles>
        </configuration>
 </plugin>
 <plugin>  
    <artifactId>maven-compiler-plugin</artifactId>
     <version>3.1</version>
    <configuration> 
       <encoding>iso-8859-1</encoding>  <source>1.7</source>  <target>1.7</target> 
	</configuration>
 </plugin> 
 <plugin>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.4</version>
