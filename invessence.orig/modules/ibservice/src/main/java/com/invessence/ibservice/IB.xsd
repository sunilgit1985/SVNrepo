<?xml version="1.0" encoding="US-ASCII"?>
<!-- $Id: IBCust_import.xsd,v 1.36 2013/04/26 14:30:28 mfornari Exp $ -->
<!-- $Source: /home/users/webdynamic/src/webdevelopment/IBCustXML/xsd/IBCust_import.xsd,v $ -->
<!-- Formatting Notes (Please Preserve on Edit): -->
<!-- Line Size: 80 -->
<!-- Tab Size: 4 spaces -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://www.interactivebrokers.com/schemas/IBCust_import"
            targetNamespace="http://www.interactivebrokers.com/schemas/IBCust_import"
            elementFormDefault="qualified" version="1.0">
	<xsd:include schemaLocation="IBCustEnumerations.xsd" />
	<xsd:element name="Applications">
		<xsd:annotation>
			<xsd:documentation>
				A set of applications belonging to a specific external entity.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Application" type="Application"
                             maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Synchronization">
		<xsd:annotation>
			<xsd:documentation>
				A set of synchronization elements.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Applications" type="NewApplications"
                             maxOccurs="1" />
				<xsd:element name="Actions" type="Actions" maxOccurs="1" />
				<xsd:element name="Additions" type="Additions" maxOccurs="1" />
				<xsd:element name="Modifications" type="Modifications"
                             maxOccurs="1" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AllDVPInstructions">
		<xsd:annotation>
			<xsd:documentation>
				For IBExecution and/or IBPrime service, DVP instructions provide
				the clear-away trading permissions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="DVPInstruction" type="DVPInstruction"
                             maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="NewApplications">
		<xsd:annotation>
			<xsd:documentation>A set of applications.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Application" type="Application"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Additions">
		<xsd:annotation>
			<xsd:documentation>
				A set of customer and customer account additions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Application" type="Application"
                         maxOccurs="unbounded" />
			<xsd:element name="Account" type="Account" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Actions">
		<xsd:annotation>
			<xsd:documentation>
				A set of customer and customer account actions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Action" type="Action" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Modifications">
		<xsd:annotation>
			<xsd:documentation>
				A set of customer and customer account additions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Customer" type="Customer" maxOccurs="unbounded" />
			<xsd:element name="Account" type="Account" maxOccurs="unbounded" />
			<xsd:element name="User" type="User" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Action">
		<xsd:annotation>
			<xsd:documentation>
				A single customer or customer account action.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="Account" type="Account" maxOccurs="unbounded" />
			<xsd:element name="User" type="User" maxOccurs="unbounded" />
			<xsd:element name="DVPInstruction" type="DVPInstruction"
                         maxOccurs="unbounded" />
		</xsd:choice>
		<xsd:attribute name="action" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="Application">
		<xsd:annotation>
			<xsd:documentation>
				An application specifies a new customer and one or more
				brokerage accounts and users.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Customer" type="Customer" maxOccurs="1" />
			<xsd:element name="Accounts" type="Accounts" maxOccurs="1" />
			<xsd:element name="Users" type="Users" maxOccurs="1" />
			<xsd:element name="Agreements" type="Agreements" minOccurs="0"
                         maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Customer identifier in the IB database (Applicant_Rtab.id)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Customer">
		<xsd:annotation>
			<xsd:documentation>
				A customer is the beneficial owner of an account. Customer
				details are specific to the customer type and expected in
				corresponding elements.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Organization" type="OrganizationApplicant"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Details of a customer of type ORG.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountHolder" type="IndividualApplicant"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Details of a customer of type INDIVIDUAL.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="JointHolders" type="JointApplicant" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Details of a customer of type JOINT.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Trust" type="TrustApplicant" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Details of a customer of type TRUST.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Customer identifier in the IB database (Applicant_Rtab.id)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Customer identifier from the external entity.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="Customer_Type" use="optional"
                       default="ORG">
			<xsd:annotation>
				<xsd:documentation>
					Type of customer according to IB's classification of
					customers (Applicant_Rtab.type). Required for non-FT loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="prefix" type="UserNamePrefix_Type"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Prefix to be used to generate the primary user name for the
					IB account of this customer. Required for non-FT loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="email" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Primary email address of the customer. Required for non-FT
					loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="md_status_nonpro" type="xsd:boolean" use="optional"
                       default="false">
			<xsd:annotation>
				<xsd:documentation>
					Trading status of the customer w.r.t Market Data
					subscriptions. Required for non-FT loads. Ignored for ORG
					customers where it is defaulted to false.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="legal_residence_country" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Country of legal residence of the customer. Only relevant
					for Non-Disclosed Broker Clients not subscribing to market
					data and therefore not providing the primary holder details
					where this is normally set.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tax_treaty_country" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Country with US Tax Treaty applicable to Customer. Only
					applicable to Non-Disclosed Broker Clients.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="meet_aml_standard" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Customer meets anti-money laundering standards. Only
					applicable to Non-Disclosed Broker Clients.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="has_direct_trading_access" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Customer have direct trading access using one of IB plataforms. Only
					applicable to Non-Disclosed Broker Clients.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="origin_country" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Country of origin of the applicant. Currently only used to
					distinguish Non-Resident Indians.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Accounts">
		<xsd:annotation>
			<xsd:documentation>
				One or more accounts to be added for a customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Account" type="Account" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Account">
		<xsd:annotation>
			<xsd:documentation>
				An account is the brokerage account which provides trading
				access, funds, and positions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="InvestmentObjectives"
                         type="InvestmentObjectivesType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BrokerageServices" type="BrokerageServices"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="Capabilities" type="Capabilities" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads, ignored for retail
						accounts. For retail accounts capabilities are derived
						from trading access requested.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TradingPermissions" type="TradingPermissions"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AllExchangeAccess" type="AllExchangeAccess"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads, ignored for retail
						accounts. For retail accounts exchange access is set
						based on qualified trading access.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DVPInstructions" type="DVPInstructions"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TradingLimits" type="TradingLimits" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdvisorWrapFees" type="AdvisorWrapFeesType"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for advisor client accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientCommissionSchedule"
                         type="CommissionScheduleType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Valid only for Broker clients, ignored for all others.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClientInterestMarkupSchedule"
                         type="InterestMarkupScheduleType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Valid only for Broker clients, ignored for all others.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Decendent" type="IRADecedent" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Details of the deceased from whom the IRA account was
						inherited. Required for inherited IRA accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IRABeneficiaries" type="IRABeneficiariesType"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						List of beneficiaries for an IRA Account. Required for
						IRA accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExtPositionsTransfer"
                         type="ExtPositionsTransferType" minOccurs="0" maxOccurs="unbounded">
			</xsd:element>
			<xsd:element name="DepositNotification"
                         type="DepositNotificationType" minOccurs="0" maxOccurs="1">
			</xsd:element>
			<xsd:element name="ACHInstruction" type="ACHInstructionType"
                         minOccurs="0" maxOccurs="unbounded">
			</xsd:element>
			<xsd:element name="RecurringTransaction"
                         type="RecurringTransactionType" minOccurs="0" maxOccurs="unbounded">
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Account Identifier in the IB database
					(CustomerAccount_Rtab.acct_id).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for the account at the external entity.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="base_currency" type="Currency_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Base Currency for the account.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="multicurrency" type="xsd:boolean" use="optional"
                       default="true">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if this account is multi-currency capable.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="margin" type="Margin_Type" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Type of margin rules to be applied to the account.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="IRA" type="xsd:boolean" use="optional"
                       default="false">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if this is an IRA account. IRA accounts
					only permitted for US Legal Residents opening an Individual
					account.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="IRA_type" type="IRA_Type" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Type of IRA account. Required if this is an IRA account.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="IRA_official_title" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required for inherited IRA if transferring from an external
					account with a matching title. The title at the transferring
					institution.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="duplicate" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the account should be cloned for this
					customer to create a duplicate account with the same
					configuration. Only applicable for Advisor Client accounts.
					In addition to trading configuration, Advisor Client Fees
					and Commission Schedules will also be duplicated. The cloned
					account will have a different account id and be accessible
					via the same user session. Cloning request will not be
					processed until the original account is approved and opened.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="no_of_duplicates" type="xsd:nonNegativeInteger"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Number of duplicate accounts required if the account is to
					be duplicated. Required if duplication is requested and
					multiple duplicates are needed. If not provided, default
					will be a single duplicate account.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="BrokerageServices">
		<xsd:annotation>
			<xsd:documentation>
				One or more brokerage services.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BrokerageService" type="BrokerageService"
                         maxOccurs="3" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TradingPermissions">
		<xsd:annotation>
			<xsd:documentation>
				One or more exchange access bundles specified by country or
				other grouping.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TradingPermission" type="TradingPermission"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AllExchangeAccess">
		<xsd:annotation>
			<xsd:documentation>
				If a trading permission bundle would provide improper exchange
				access, then specify exactly a set of exchanges.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExchangeAccess" type="ExchangeAccess"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Capabilities">
		<xsd:annotation>
			<xsd:documentation>
				A set of capabilities will be inferred from the trading
				permissions. However, additional capabilities can be specified.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Capability" type="Capability"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DVPInstructions">
		<xsd:annotation>
			<xsd:documentation>
				For IBExecution and/or IBPrime service, DVP instructions provide
				the clear-away trading permissions.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DVPInstruction" type="DVPInstruction"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Users">
		<xsd:annotation>
			<xsd:documentation>
				One or more users can be specified.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="User" type="User" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="User">
		<xsd:annotation>
			<xsd:documentation>
				A user is an individual with specific access to one or more
				customer accounts. A user must be linked to an individual
				through a specified id.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UserPrivilege" type="UserPrivilege" minOccurs="0"
                         maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Ignored for the first/primary user who is automatically
						set up with the OWNER role.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MDServices" type="MDServicesType" minOccurs="0"
                         maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for a user in the IB database (IBUser_Rtab.id).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_user_id" type="xsd:string"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for the user at the external entity.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_individual_id" type="xsd:string"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Identifier at the external entity for the individual
					associated with this user. Required to create the
					association within the IB database.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="prefix" type="UserNamePrefix_Type"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Prefix to be used to generate the IB username for the user.
					Required only for additional users. Ignored for the
					first/primary user, the username generated for the id_string
					of the customer is automatically set as the username for the
					first/primary user.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="UserPrivilege">
		<xsd:annotation>
			<xsd:documentation>
				A user is privileged for a specified account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="external_acct_id" type="xsd:string" use="required" />
		<xsd:attribute name="privilege" type="Privilege_Type" use="required" />
	</xsd:complexType>
	<xsd:complexType name="DVPInstruction">
		<xsd:annotation>
			<xsd:documentation>
				A DVP instruction specifies the settlement information for
				clear-away trading.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="id" type="xsd:string" use="optional" />
		<xsd:attribute name="external_id" type="xsd:string" use="required" />
		<xsd:attribute name="ExternalAcctID" type="xsd:string" />
		<xsd:attribute name="AcctID" type="xsd:string" />
		<xsd:attribute name="Name" type="xsd:string" use="required" />
		<xsd:attribute name="Type" type="DVP_Type" use="required" />
		<xsd:attribute name="Role" type="DVPRole_Type" use="required" />
		<xsd:attribute name="AgentID" type="xsd:string" use="required" />
		<xsd:attribute name="FirmID" type="xsd:string" use="required" />
		<xsd:attribute name="AccountID" type="xsd:string" use="required" />
		<xsd:attribute name="AgentName" type="xsd:string" use="required" />
		<xsd:attribute name="AccountName" type="xsd:string" use="required" />
		<xsd:attribute name="DayDoID" type="xsd:string" use="required" />
		<xsd:attribute name="TXGroupCode" type="TXGroupCode_Type"
                       use="required" />
		<xsd:attribute name="BrokerCode" type="xsd:string" use="required" />
		<xsd:attribute name="AssetClass" type="AssetClass_Type" use="required" />
		<xsd:attribute name="Exchange" type="ExchangeCode_Type" use="required" />
		<xsd:attribute name="PrepayTax" type="xsd:boolean" use="optional"
                       default="false" />
		<xsd:attribute name="PrepayCommission" type="xsd:boolean" use="optional"
                       default="false" />
		<xsd:attribute name="Default" type="xsd:boolean" use="optional"
                       default="true" />
		<xsd:attribute name="Expiry" type="xsd:date" use="required" />
	</xsd:complexType>
	<xsd:complexType name="OrganizationApplicant">
		<xsd:annotation>
			<xsd:documentation>
				An organization which is the customer and beneficial owner of an
				account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="OrganizationIdentification"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="AccountSupport" type="AccountSupportType"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="FinancialInformation" type="FinancialInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccreditedInvestorInformation"
                         type="AccreditedInvestorInformation" minOccurs="0" maxOccurs="1" />
			<xsd:element name="RegulatoryInformation"
                         type="RegulatoryInformation" minOccurs="1" maxOccurs="1" />
			<xsd:element name="PrimaryTrader" type="PrimaryTrader" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ControllingOfficer" type="ControllingOfficer"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AssociatedEntities" type="AssociatedEntities"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="RegulatedMemberships" type="RegulatedMemberships"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type_of_trading" type="TradingFor_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Nature of trading to be carried out by the organization
					using this account. Current support limited to Firm trading
					for non-FT loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="OrgApplicant_Type" use="optional"
                       default="CORPORATION">
			<xsd:annotation>
				<xsd:documentation>
					Type of organization (OrganizationApplicant_Rtab.type).
					Required for non-FT loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="IndividualApplicant">
		<xsd:annotation>
			<xsd:documentation>
				An individual who is the customer and beneficial owner of an
				account. Ownership and Title are fixed at 100% and Account
				Holder respectively and provided values will be ignored.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AccountHolderDetails" type="AssociatedIndividual"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="FinancialInformation" type="FinancialInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegulatoryInformation"
                         type="RegulatoryInformation" minOccurs="1" maxOccurs="1" />
			<xsd:element name="RegulatedMemberships" type="RegulatedMemberships"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccreditedInvestorInformation"
                         type="AccreditedInvestorInformation" minOccurs="0" maxOccurs="1" />
			<xsd:element name="TaxInformation" type="IndividualTaxInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="JointApplicant">
		<xsd:annotation>
			<xsd:documentation>
				Pair of individuals who are the joint customers and joint
				beneficial owners of an account. Only Joint applications of type
				tennants common may specify ownership for the holders, for the
				rest this is ignored.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="FirstHolderDetails" type="AssociatedIndividual"
                         minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Title is fixed as First Holder, provided value is
						ignored.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SecondHolderDetails" type="AssociatedIndividual"
                         minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Title is fixed as Second Holder, provided value is
						ignored.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FinancialInformation" type="FinancialInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegulatoryInformation"
                         type="RegulatoryInformation" minOccurs="1" maxOccurs="1" />
			<xsd:element name="RegulatedMemberships" type="RegulatedMemberships"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccreditedInvestorInformation"
                         type="AccreditedInvestorInformation" minOccurs="0" maxOccurs="1" />
			<xsd:element name="TaxInformation" type="IndividualTaxInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="Joint_Type" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Type of Joint customer (JointApplicant_Rtab.type)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="TrustApplicant">
		<xsd:annotation>
			<xsd:documentation>
				A Trust which is the customer and beneficial owner of an
				account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="TrustIdentification"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="FinancialInformation" type="FinancialInformation"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for non-FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RegulatoryInformation"
                         type="RegulatoryInformation" minOccurs="1" maxOccurs="1" />
			<xsd:element name="RegulatedMemberships" type="RegulatedMemberships"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Only supported for FT loads.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccreditedInvestorInformation"
                         type="AccreditedInvestorInformation" minOccurs="0" maxOccurs="1" />
			<xsd:element name="Trustees" type="TrusteesType" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="Beneficiaries" type="AssociationTypeEntities"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="Grantors" type="AssociationTypeEntities"
                         minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="third_party_mgmt" type="xsd:boolean"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the account will be accessed by a third
					party advisor or professional manager. If so, the customer
					market data status will be defaulted to professional per
					Exchange requirements.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="OrganizationIdentification">
		<xsd:annotation>
			<xsd:documentation>
				The organization name will be the account title for cashiering
				functions. Otherwise, this information is compliance mandated.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PlaceOfBusiness" type="Address">
				<xsd:annotation>
					<xsd:documentation>
						Official address of the organization.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MailingAddress" type="Address" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Mailing address of the organization if different from
						the place of business. Required if flag indicating a
						different mailing address is set.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phones" type="IndividualPhones" minOccurs="0"
                         maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required" />
		<xsd:attribute name="business_description" type="xsd:string"
                       use="required" />
		<xsd:attribute name="identification" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Standard Official Identification for the organization. For a
					US organization this will be its Tax ID.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="identification_country" type="CountryCode_Type"
                       use="required" />
		<xsd:attribute name="formation_country" type="CountryCode_Type"
                       use="required" />
		<xsd:attribute name="formation_state" type="StateCode_Type"
                       use="required" />
		<xsd:attribute name="same_mail_address" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate that the customer mailing address is the
					same as its place of business.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="LegalEntityIdentification">
		<xsd:annotation>
			<xsd:documentation>
				Partial organization identification information. For associated
				legal entities of an organizational customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PlaceOfBusiness" type="Address" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Official address of the entity.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MailingAddress" type="Address" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Mailing address of the entity if different from the
						place of business. Required if flag indicating a
						different mailing address is set.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="identification" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Standard Official Identification for the entity. For a US
					legal entity this will be its Tax ID.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="identification_country" type="xsd:string"
                       use="required" />
		<xsd:attribute name="formation_country" type="CountryCode_Type"
                       use="required" />
		<xsd:attribute name="formation_type" type="Organization_Type"
                       use="required" />
		<xsd:attribute name="exchange_code" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Standard code for the exchange on which this entity is
					listed. Required for an entity of formation_type public.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="exchange_symbol" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Symbol of the entity's listing on the exchange. Required if
					entity formation_type is public.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="same_mail_address" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate that the entity mailing address is the same
					as its place of business.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="TrustIdentification">
		<xsd:annotation>
			<xsd:documentation>
				The Trust name will be the account title for cashiering
				functions. Otherwise, this information is compliance mandated.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Address" type="Address">
				<xsd:annotation>
					<xsd:documentation>
						Official address of the Trust.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MailingAddress" type="Address" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Mailing address of the Trust if different from the
						official address. Required if flag indicating a
						different mailing address is set.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phones" type="IndividualPhones" />
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required" />
		<xsd:attribute name="type_of_trust" type="xsd:string" use="required" />
		<xsd:attribute name="purpose_of_trust" type="xsd:string" use="required" />
		<xsd:attribute name="date_formed" type="DOB" use="required" />
		<xsd:attribute name="formation_country" type="CountryCode_Type"
                       use="required" />
		<xsd:attribute name="formation_state" type="StateCode_Type"
                       use="required" />
		<xsd:attribute name="registration_number" type="xsd:string"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Registration Number for the Trust. For a US Trust this will
					be its Tax ID.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="registration_country" type="CountryCode_Type"
                       use="required" />
		<xsd:attribute name="same_mail_address" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate that the customer mailing address is the
					same as its place of business.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="AssociatedEntities">
		<xsd:annotation>
			<xsd:documentation>
				One or more individuals or legal entities that are associated
				with the customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AssociatedIndividual" type="AssociatedIndividual"
                         minOccurs="1" maxOccurs="9" />
			<xsd:element name="AssociatedEntity" type="AssociatedEntity"
                         minOccurs="0" maxOccurs="9" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IRABeneficiariesType">
		<xsd:annotation>
			<xsd:documentation>
				One or more individuals or legal entities who are the primary
				and contingent beneficiaries of the IRA account. At least one
				primary beneficiary, individual or legal entity, must be
				provided. Net ownership of primary and contingent beneficiaries
				must add up to 100% for each beneficary type.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PrimaryBeneficiary" type="IRAPrimaryBeneficiary"
                         minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="PrimaryBeneficiaryEntity"
                         type="IRAPrimaryBeneficiaryEntity" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="ContingentBeneficiary"
                         type="IRAContingentBeneficiary" minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="ContingentBeneficiaryEntity"
                         type="IRAContingentBeneficiaryEntity" minOccurs="0" maxOccurs="unbounded" />
		</xsd:sequence>
		<xsd:attribute name="spouse_primary_beneficary" type="xsd:boolean"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the spouse is the primary beneficiary of
					the IRA account. Required for a married account holder. If
					spouse is not the primary beneficiary then a signed spousal
					consent is required.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="AssociationTypeEntities">
		<xsd:annotation>
			<xsd:documentation>
				One or more individuals or legal entities with a specific
				association with the customer as defined by the declaring
				element.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Individual" type="Individual" minOccurs="0"
                         maxOccurs="unbounded" />
			<xsd:element name="LegalEntity" type="LegalEntity" minOccurs="0"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TrusteesType">
		<xsd:annotation>
			<xsd:documentation>
				One or more individuals or legal entities that are the trustees
				of the Trust.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Individual" type="Individual" minOccurs="0"
                         maxOccurs="unbounded" />
			<xsd:element name="Entity" type="TrusteeEntityType" minOccurs="0"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TrusteeEntityType">
		<xsd:annotation>
			<xsd:documentation>
				A legal entity that is the trustee of a trust. An entity trustee
				must detail an employee signatory.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="LegalEntity" type="LegalEntity" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="Employee" type="Individual" minOccurs="1"
                         maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FinancialInformation">
		<xsd:annotation>
			<xsd:documentation>
				Financial information about the customer required to qualify
				them for trading and brokerage services. All amount values must
				be in USD.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="InvestmentExperience"
                         type="InvestmentExperience" />
			<xsd:element name="InvestmentObjectives"
                         type="InvestmentObjectivesType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Deprecated. Should be provided individually for each
						account being added for the customer using the
						InvestmentObjectives sub-element of an Account element.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="net_worth" type="xsd:decimal" use="required" />
		<xsd:attribute name="liquid_net_worth" type="xsd:decimal"
                       use="required" />
		<xsd:attribute name="annual_net_income" type="xsd:decimal"
                       use="required" />
		<xsd:attribute name="total_assets" type="xsd:decimal" use="required" />
		<xsd:attribute name="source_of_funds" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required for Organizations and Trusts and for Individuals
					noting Unemployed/Retired as Employment Type.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="InvestmentExperience">
		<xsd:annotation>
			<xsd:documentation>
				The customer's experience and knowledge with the various
				financial instruments required to qualify trading in those
				instruments.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AssetExperience" type="AssetExperience"
                         maxOccurs="5" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AssetExperience">
		<xsd:annotation>
			<xsd:documentation>
				Trading experience and knowledge for a specific financial
				instrument.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="asset_class" type="AssetClass_Type" use="required" />
		<xsd:attribute name="years_trading" type="xsd:nonNegativeInteger"
                       use="required" />
		<xsd:attribute name="trades_per_year" type="xsd:nonNegativeInteger"
                       use="required" />
		<xsd:attribute name="knowledge_level" type="KnowledgeLevel_Type"
                       use="required" />
	</xsd:complexType>
	<xsd:complexType name="AccreditedInvestorInformation">
		<xsd:annotation>
			<xsd:documentation>
				Our five accredited investor questions. See a printed
				application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="q1" type="xsd:boolean" use="optional"
                       default="true" />
		<xsd:attribute name="q2" type="xsd:boolean" use="optional"
                       default="true" />
		<xsd:attribute name="q3" type="xsd:boolean" use="optional"
                       default="true" />
		<xsd:attribute name="q4" type="xsd:boolean" use="optional"
                       default="true" />
		<xsd:attribute name="q5" type="xsd:boolean" use="optional"
                       default="true" />
	</xsd:complexType>
	<xsd:complexType name="RegulatoryInformation">
		<xsd:annotation>
			<xsd:documentation>
				All information which is required for compliance reasons. The
				attribute names are codes for the various types of organizations
				that the customer must declare.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
                  <xsd:choice>
			      <xsd:element name="RegulatoryDetails" type="RegulatoryDetail"
                               maxOccurs="unbounded" />
			      <xsd:element name="RegulatoryDetail" type="RegulatoryDetail"
                               maxOccurs="unbounded" />
                  </xsd:choice>
			<xsd:element name="SelfRegulatedMembership"
                         type="SelfRegulatedMembershipType" minOccurs="0" maxOccurs="1" />
			<xsd:element name="AffiliationDetails" type="AffiliationDetailsType"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="FinancialOrgType" type="FinancialOrgType"
                         minOccurs="0" maxOccurs="unbounded" />
			<xsd:element name="ORGRegulatoryInfo" type="ORGRegulatoryInfoType"
                         minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegulatoryDetail">
		<xsd:annotation>
			<xsd:documentation>
				Specific questions about regulatory issues. An enumeration of
				question codes will be declared. See a paper application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="code" type="RegulatoryIssueCode_Type"
                       use="required" />
		<xsd:attribute name="status" type="xsd:boolean" use="optional"
                       default="false" />
		<xsd:attribute name="details" type="xsd:string" use="optional" />
            <xsd:attribute name="detail" type="xsd:string" use="optional" />
	</xsd:complexType>
	<xsd:complexType name="SelfRegulatedMembershipType">
		<xsd:annotation>
			<xsd:documentation>
				List of exchanges and self regulated organizations the customer
				is a member of.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="exchanges" type="xsd:string" minOccurs="0"
                         maxOccurs="1" default="" />
			<xsd:element name="organizations" type="xsd:string" minOccurs="0"
                         maxOccurs="1" default="" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AffiliationDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Details of customer or customer household member affiliation to
				a broker dealer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="affiliation_relationship" type="xsd:string"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="person_name" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="company" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="company_mailing_address" type="Address"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="company_phone" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FinancialOrgType">
		<xsd:annotation>
			<xsd:documentation>
				Specific questions about financial org types. See a paper
				application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="code" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="RegulatedMemberships">
		<xsd:annotation>
			<xsd:documentation>
				One or more regulated memberships.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RegulatedMembership" type="RegulatedMembership"
                         maxOccurs="9" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="RegulatedMembership">
		<xsd:annotation>
			<xsd:documentation>
				We must know of the membership in any regulated organizations.
				E.g. an NYSE or NASD member.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="organization_code" type="xsd:string" use="optional" default="NASD" />
		<xsd:attribute name="membership_id" type="xsd:string" use="optional" default="IBKR" />
    </xsd:complexType>
	<xsd:complexType name="PublicCompanyInfoType">
      <xsd:sequence>
        <xsd:element name="ExchangeTradedOn" type="xsd:string" minOccurs="1" maxOccurs="1" default="" />
        <xsd:element name="QuotedSymbol" type="xsd:string"	minOccurs="1" maxOccurs="1" default="" />
      </xsd:sequence>
    </xsd:complexType>
	<xsd:complexType name="ORGRegulatorType">
      <xsd:sequence>
        <xsd:element name="RegulatorName" type="xsd:string"	minOccurs="1" maxOccurs="1" default="" />
        <xsd:element name="RegulatorCountry" type="xsd:string"	minOccurs="1" maxOccurs="1" default="" />
        <xsd:element name="RegulatedInCapacity" type="xsd:string" minOccurs="1" maxOccurs="1" default="" />
        <xsd:element name="RegulatorId" type="xsd:string" minOccurs="1" maxOccurs="1" default="" />
      </xsd:sequence>
    </xsd:complexType>
	<xsd:complexType name="ORGRegulatoryInfoType">
      <xsd:sequence>
        <xsd:element name="PublicCompanyInfo" type="PublicCompanyInfoType" minOccurs="0" maxOccurs="1" />
        <xsd:element name="ORGRegulator" type="ORGRegulatorType" minOccurs="0" maxOccurs="3" />
      </xsd:sequence>
      <xsd:attribute name="is_public" type="xsd:boolean" use="optional" default="false" />
      <xsd:attribute name="is_regulated" type="xsd:boolean" use="optional" default="false" />
	</xsd:complexType>
	<xsd:complexType name="AssociatedIndividual">
		<xsd:annotation>
			<xsd:documentation>
				An individual who is associated with this customer and
				qualification of the association.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PrimaryTrader">
		<xsd:annotation>
			<xsd:documentation>
				The individual who will be associated with the trading session
				for the Organization's account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="1" maxOccurs="1" />
                   <!-- application expects value = TRADER -->
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ControllingOfficer">
		<xsd:annotation>
			<xsd:documentation>
				The CEO of the customer's organization.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="1" maxOccurs="1"/>
                   <!-- application expects value = CEO -->
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IRAPrimaryBeneficiary">
		<xsd:annotation>
			<xsd:documentation>
				The individual who is the primary beneficiary of the IRA
				account. Here the Ownership denotes the beneficiary's share.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0"
                                 maxOccurs="1"/> <!-- application expects value = IRA_BENEFICIARY -->
                   <xsd:element name="Relationship"
                                type="IRABeneficiaryRelation_Type" minOccurs="1" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IRAPrimaryBeneficiaryEntity">
		<xsd:annotation>
			<xsd:documentation>
				The Legal Entity that is the primary beneficiary of the IRA
				account. Here the Ownership denotes the beneficiary's share.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="SimpleLegalEntityType">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0"
                                 maxOccurs="1" /> <!-- application expects value = IRA_BENEFICIARY -->
                   <xsd:element name="Relationship"
                                type="IRABeneficiaryRelation_Type" minOccurs="1" maxOccurs="1" />
					<xsd:element name="Executor" type="Individual" minOccurs="0"
                                 maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Details of the Trustee/Executor where the
								beneficiary is a Trust. Only considered if the
								beneficiary specified is a Trust, then too,
								optional. Only the name is required, address may
								optionally be provided.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExecutionDate" type="xsd:date"
                                 minOccurs="1" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Date of execution of the Trust or Will.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ArticleOfWill" type="xsd:string"
                                 minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Article of the Will. Required for Trust
								Beneficiaries.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IRAContingentBeneficiary">
		<xsd:annotation>
			<xsd:documentation>
				The individual who is the contingent beneficiary of the IRA
				account. Here the Ownership denotes the beneficiary's share.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0"
                                 maxOccurs="1" /> <!-- application expects value = CONTINGENT -->
                   <xsd:element name="Relationship"
                                type="IRABeneficiaryRelation_Type" minOccurs="1" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IRAContingentBeneficiaryEntity">
		<xsd:annotation>
			<xsd:documentation>
				The Legal Entity that is the contingent beneficiary of the IRA
				account. Here the Ownership denotes the beneficiary's share.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="SimpleLegalEntityType">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0"
                                 maxOccurs="1" />  <!-- application expects value = CONTINGENT -->
                   <xsd:element name="Relationship"
                                type="IRABeneficiaryRelation_Type" minOccurs="1" maxOccurs="1" />
					<xsd:element name="Executor" type="Individual" minOccurs="0"
                                 maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Details of the Trustee/Executor where the
								beneficiary is a Trust. Only considered if the
								beneficiary specified is a Trust, then too,
								optional. Only the name is required, address may
								optionally be provided.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExecutionDate" type="xsd:date"
                                 minOccurs="1" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Date of execution of the Trust or Will.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ArticleOfWill" type="xsd:string"
                                 minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>
								Article of the Will. Required for Trust
								Beneficiaries.
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IRADecedent">
		<xsd:annotation>
			<xsd:documentation>
				The deceased individual from whom the IRA account is being
				inherited. Required information for a decedent is the name, date
				of birth, date of death, SSN and the inheritor type.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Individual">
				<xsd:sequence>
					<xsd:element name="DateOfDeath" type="DOB" minOccurs="1"
                                 maxOccurs="1" />
					<xsd:element name="Title" type="Title" minOccurs="1"
                                 maxOccurs="1" /> <!-- application expects value = IRA_DECEDENT -->
                   <xsd:element name="InheritorType"
                                type="InheritedIRABeneficiary_Type" minOccurs="1" maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AssociatedEntity">
		<xsd:annotation>
			<xsd:documentation>
				A legal entity which is associated with this customer and
				qualification of the association.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="LegalEntity">
				<xsd:sequence>
					<xsd:element name="Ownership" type="Ownership" />
					<xsd:element name="Title" type="Title" minOccurs="0"
                                 maxOccurs="1" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Individual">
		<xsd:annotation>
			<xsd:documentation>
				A general individual. Identification information is mandatory.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="IndividualName" />
			<xsd:element name="DOB" type="DOB" />
			<xsd:element name="Gender" type="Gender_Type" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="MaritalStatus" type="MaritalStatus_Type"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for individuals associated with Individual or
						Joint accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumDependents" type="xsd:nonNegativeInteger"
                         minOccurs="0" maxOccurs="1" default="0">
				<xsd:annotation>
					<xsd:documentation>
						Required for individuals associated with Individual or
						Joint accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Residence" type="Address" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="MailingAddress" type="Address" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="Phones" type="IndividualPhones" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="Email" type="Email" minOccurs="0" maxOccurs="1" />
			<xsd:element name="Identification" type="IndividualIdentification" minOccurs="0" maxOccurs="1" />
			<xsd:element name="EmploymentType" type="Employment_Type"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for individuals associated with Individual or
						Joint accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmploymentDetails" type="EmploymentDetailsType"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for individuals associated with Individual or
						Joint accounts that specify Employed or Self-employed as
						theyir employment type.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EmployeeTitle" type="xsd:string" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Job title of the individual at his employer. Required
						for individuals associated with Organization accounts.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for the individual in the IB database
					(Individual_Rtab.id).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for the individual at the external entity.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="same_mail_address" type="xsd:boolean"
                       use="optional" default="true">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the individual's mailing address is the
					same as the residential address.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="LegalEntity">
		<xsd:annotation>
			<xsd:documentation>
				A legal entity is anything other than an individual.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="EntityName" />
			<xsd:element name="Location" type="Address" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Deprecated. Entity's primary address must be noted via
						its place of business in the identification element.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Phones" type="IndividualPhones" />
			<xsd:element name="Email" type="Email" />
			<xsd:element name="LegalEntityIdentification"
                         type="LegalEntityIdentification" />
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional" />
		<xsd:attribute name="external_id" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="Ownership">
		<xsd:annotation>
			<xsd:documentation>
				Defines ownership with a specified percentage.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="percentage" type="xsd:nonNegativeInteger"
                       use="optional" default="0" />
	</xsd:complexType>
	<xsd:complexType name="Title">
		<xsd:annotation>
			<xsd:documentation>
				Nature of association of an individual or entity with the
				customer account.
			</xsd:documentation>
		</xsd:annotation>
            <xsd:simpleContent>
                  <xsd:extension base="xsd:string">
		           <xsd:attribute name="code" type="Association_Type" use="optional" />
                  </xsd:extension>
            </xsd:simpleContent>
	</xsd:complexType>
	<xsd:complexType name="IndividualName">
		<xsd:annotation>
			<xsd:documentation>
				An individual's name as components.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="salutation" type="Salutation_Type" use="optional" />
		<xsd:attribute name="first" type="xsd:string" use="required" />
		<xsd:attribute name="last" type="xsd:string" use="required" />
		<xsd:attribute name="middle" type="xsd:string" use="optional" />
		<xsd:attribute name="suffix" type="xsd:string" use="optional" />
		<xsd:attribute name="title" type="xsd:string" use="optional" />
	</xsd:complexType>
	<xsd:complexType name="IndividualPhones">
		<xsd:annotation>
			<xsd:documentation>
				Direct contact details for an individual via Phone and Fax.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="primary" type="xsd:string" use="required" />
		<xsd:attribute name="secondary" type="xsd:string" use="optional" />
		<xsd:attribute name="fax" type="xsd:string" use="optional" />
	</xsd:complexType>
	<xsd:complexType name="IndividualIdentification">
		<xsd:annotation>
			<xsd:documentation>
				All possible forms of indentification for an individual. SSN is
				required for US citizens and legal residents. For non-US
				customers, at least one form of identification must be provided.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="citizenship" type="xsd:string" use="required" />
		<xsd:attribute name="SSN" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Must be numeric, 9 digits no dashes.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="DriversLicense" type="xsd:string" use="optional" />
		<xsd:attribute name="Passport" type="xsd:string" use="optional" />
		<xsd:attribute name="AlienCard" type="xsd:string" use="optional" />
		<xsd:attribute name="NationalCard" type="xsd:string" use="optional" />
		<xsd:attribute name="IssuingCountry" type="xsd:string" use="optional" />
		<xsd:attribute name="LegalResidenceCountry" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required for non-FT loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="LegalResidenceState" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required for US legal residents.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Email">
		<xsd:annotation>
			<xsd:documentation>
				Means for us to spam a customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="email" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="Security">
		<xsd:annotation>
			<xsd:documentation>
				For customer identification by telephone. challenge/response,
				aka hint/phrase, aka question/answer
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="Challenge" type="xsd:string" use="required" />
		<xsd:attribute name="Response" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:simpleType name="DOB">
		<xsd:annotation>
			<xsd:documentation>
				A date specifically for a birthday.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:date"></xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="Address">
		<xsd:annotation>
			<xsd:documentation>A general street address.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="street_1" type="xsd:string" use="required" />
		<xsd:attribute name="street_2" type="xsd:string" use="optional" />
		<xsd:attribute name="city" type="xsd:string" use="required" />
		<xsd:attribute name="state" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					For US and Canadian states, the ISO 2 character code. For
					Indian states, the ISO IN-2 character code.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="country" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					The full country name not its ISO 2 or 3 character code.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="postal_code" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="EntityName">
		<xsd:annotation>
			<xsd:documentation>A legal entity's name.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:string" use="required" />
	</xsd:complexType>
	<xsd:complexType name="Capability">
		<xsd:annotation>
			<xsd:documentation>An ACES account capability.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="code" type="Capability_Type" use="required" />
	</xsd:complexType>
	<xsd:complexType name="BrokerageService">
		<xsd:annotation>
			<xsd:documentation>
				An ACES account brokerage service.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="code" type="BrokerageService_Type" use="required" />
	</xsd:complexType>
	<xsd:complexType name="TradingPermission">
		<xsd:annotation>
			<xsd:documentation>
				An ACES account trading bundle.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="asset_class" type="AssetClass_Type"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Deprecated. Not required as a exchange bundle comprises of
					combinations of exchange and asset class.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="exchange_group" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Code for a bundle of trading permissions
					(ExchangeBundle_Rtab.code).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ExchangeAccess">
		<xsd:annotation>
			<xsd:documentation>
				An ACES account exchange access.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="asset_class" type="AssetClass_Type" use="required" />
		<xsd:attribute name="exchange" type="ExchangeCode_Type" use="required" />
	</xsd:complexType>
	<xsd:complexType name="AssetClass">
		<xsd:annotation>
			<xsd:documentation>
				An ACES asset class, aka financial instrument type.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="code" type="AssetClass_Type" use="required" />
	</xsd:complexType>
	<xsd:complexType name="TradingLimits">
		<xsd:annotation>
			<xsd:documentation>
				Various limits for the llimit tracking mechanism.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OrderValueLimits" type="OrderValueLimits" />
			<xsd:element name="EFPQuantityLimits" type="EFPQuantityLimits" />
			<xsd:element name="OrderQuantityLimit" type="OrderQuantityLimit"
                         maxOccurs="9" />
			<xsd:element name="DayQuantityLimit" type="DayQuantityLimit"
                         maxOccurs="9" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="OrderValueLimits">
		<xsd:annotation>
			<xsd:documentation>Per day order value limits.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="max_order_value" type="xsd:decimal" use="required" />
		<xsd:attribute name="max_gross_value" type="xsd:decimal" use="required" />
		<xsd:attribute name="max_net_value" type="xsd:decimal" use="required" />
		<xsd:attribute name="net_contract_limit" type="xsd:decimal"
                       use="required" />
	</xsd:complexType>
	<xsd:complexType name="EFPQuantityLimits">
		<xsd:annotation>
			<xsd:documentation>EFP order quantity limits.</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="max_nominal_efp_per_order"
                       type="xsd:nonNegativeInteger" use="required" />
		<xsd:attribute name="max_net_efp_trades" type="xsd:nonNegativeInteger"
                       use="required" />
		<xsd:attribute name="max_gross_efp_trades" type="xsd:nonNegativeInteger"
                       use="required" />
	</xsd:complexType>
	<xsd:complexType name="OrderQuantityLimit">
		<xsd:annotation>
			<xsd:documentation>
				Per asset class order quantity limit.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="asset" type="AssetClass_Type" use="required" />
		<xsd:attribute name="quantity" type="xsd:nonNegativeInteger"
                       use="required" />
	</xsd:complexType>
	<xsd:complexType name="DayQuantityLimit">
		<xsd:annotation>
			<xsd:documentation>
				Per asset class day quantity limit.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="asset" type="AssetClass_Type" use="required" />
		<xsd:attribute name="quantity" type="xsd:nonNegativeInteger"
                       use="required" />
	</xsd:complexType>
	<xsd:simpleType name="UserNamePrefix_Type">
		<xsd:annotation>
			<xsd:documentation>
				Prefix used to generate a username.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[a-z]{5}" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="MDServicesType">
		<xsd:annotation>
			<xsd:documentation>
				List of Market Data Services the user is requesting subscription
				to.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="service" type="xsd:nonNegativeInteger"
                         minOccurs="1" maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InvestmentObjectivesType">
		<xsd:annotation>
			<xsd:documentation>
				Investment objectives of the customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="objective" type="InvestmentObjective_Type"
                         minOccurs="1" maxOccurs="6" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EmploymentDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Employment details of the customer.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="employer" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="occupation" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="employer_business" type="xsd:string"
                         minOccurs="1" maxOccurs="1" />
			<xsd:element name="employer_address" type="Address" minOccurs="1"
                         maxOccurs="1" />
			<xsd:element name="employer_phone" type="xsd:string" minOccurs="1"
                         maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AdvisorWrapFeesType">
		<xsd:annotation>
			<xsd:documentation>
				Advisor Wrap Fee details for a client account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="automated_fees_details" type="AutomatedWrapFeeDetailsType" minOccurs="0" maxOccurs="unbounded" />
		        <xsd:element name="highWaterMarkConfigHwma" type="HighWaterMarkType" maxOccurs="1" minOccurs="0">
				<xsd:annotation>
				   <xsd:documentation>
				      Required if High Water Mark is set for PERCENTOFPROFIT.
				   </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="highWaterMarkConfigHwmq" type="HighWaterMarkType" maxOccurs="1" minOccurs="0">
			      <xsd:annotation>
			           <xsd:documentation>
			                 Required if High Water Mark is set for PERCENTOFPROFIT_QUARTER.
			           </xsd:documentation>
			      </xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="strategy" type="AdvisorWrapFeeStrategies_Type"
                       use="optional" default="NO_FEES" />
	</xsd:complexType>
	<xsd:complexType name="AutomatedWrapFeeDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Details of an Automated Advisor Wrap Fee Strategy for a Client.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="per_trade_markups" type="CommissionScheduleType"
                         minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required if the Automated Wrap Fee type is PERTRADE.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="AutomatedWrapFees_Type" use="required" />
		<xsd:attribute name="max_fee" type="xsd:decimal" use="optional"
                       default="0" />
		<xsd:attribute name="num_contracts" type="xsd:nonNegativeInteger"
                       use="optional" default="1" />
	</xsd:complexType>
	<xsd:complexType name="HighWaterMarkType">
		<xsd:annotation>
			<xsd:documentation>
				HighWaterMark Setup.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="hwm" type="HighWaterMarkConfigurationType" minOccurs="1" 	maxOccurs="1" />
		        <xsd:element name="previousLosses" type="PreviousLossesType" minOccurs="0" 	maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="HighWaterMarkConfigurationType">
		<xsd:annotation>
			<xsd:documentation>
				HighWaterMark Configuration.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="NumberOfPeriods" type="xsd:nonNegativeInteger" use="required" />
		<xsd:attribute name="ProrateForWithdrawals" type="xsd:boolean" use="optional" default="false" />
	</xsd:complexType>
	<xsd:complexType name="PreviousLossesType">
		<xsd:annotation>
			<xsd:documentation>
				Previous Losses for HighWaterMark.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="loss" type="xsd:nonNegativeInteger" use="required"/>
	      <xsd:attribute name="quarter" type="xsd:nonNegativeInteger" use="optional" />
	      <xsd:attribute name="year" type="xsd:nonNegativeInteger" use="required" />
            <xsd:attribute name="currency" type="xsd:string" use="required"/>
	</xsd:complexType>

	<xsd:complexType name="CommissionScheduleType">
		<xsd:annotation>
			<xsd:documentation>
				Schedule of commission markups.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="markup" type="CommissionMarkupType" minOccurs="1"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CommissionMarkupType">
		<xsd:annotation>
			<xsd:documentation>
				Specific markup in a commission schedule.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="stair" type="MarkupStaircaseType" minOccurs="0"
                         maxOccurs="unbounded" />
		</xsd:sequence>
		<xsd:attribute name="code" type="xsd:string" use="required" />
		<xsd:attribute name="minimum" type="xsd:decimal" use="optional" />
                <xsd:attribute name="maximum" type="xsd:decimal" use="optional" />
		<xsd:attribute name="type" type="CommissionMarkups_Type" use="required" />
		<xsd:attribute name="amount" type="xsd:decimal" use="optional" />
		<xsd:attribute name="plus_cost" type="xsd:boolean" use="optional"
                       default="false" />
	</xsd:complexType>
	<xsd:complexType name="MarkupStaircaseType">
		<xsd:annotation>
			<xsd:documentation>
				A break in the markup staircase of a commission schedule.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="break" type="xsd:decimal" use="optional" />
		<xsd:attribute name="amount" type="xsd:decimal" use="required" />
	</xsd:complexType>
	<xsd:complexType name="InterestMarkupScheduleType">
		<xsd:annotation>
			<xsd:documentation>
				Schedule of interest rate markups.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="markup" type="InterestMarkupType" minOccurs="1"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InterestMarkupType">
		<xsd:annotation>
			<xsd:documentation>
				Interest Rate markups for a specific currency.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="currency" type="Currency_Type" use="required" />
		<xsd:attribute name="debit_markup" type="xsd:decimal" use="optional" />
		<xsd:attribute name="credit_markdown" type="xsd:decimal" use="optional" />
		<xsd:attribute name="short_credit_markdown" type="xsd:decimal"
                       use="optional" />
	</xsd:complexType>
	<xsd:element name="ExtPositionsTransfers">
		<xsd:annotation>
			<xsd:documentation>
				Details for transferring positions from a third party broker to
				IB using a specific transfer mechanism such as ACATS or ATON for
				one or more customer accounts at IB.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ExtPositionsTransfer"
                             type="ExtPositionsTransferType" minOccurs="1" maxOccurs="unbounded">
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="ExtPositionsTransferType">
		<xsd:annotation>
			<xsd:documentation>
				Details for transferring positions from a third party broker to
				IB using a specific transfer mechanism such as ACATS or ATON.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="type" type="PositionsTransfer_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Scope of transfer being requested, full or partial.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="sub_type" type="PositionsTransfer_SubType"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Type of transfer being requested, ACATS, ATON, etc.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="broker_id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Industry standard Id for the broker from whom the positions
					need to be transfered.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="broker_name" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Name for the broker from whom the positions need to be
					transfered as known to IB.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="acct_at_broker" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Customer Account at the third party broker from which to
					transfer positions
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="src_IRA_type" type="IRA_Type" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					IRA type of the account at the third party broker from which
					positions are to be transferred to an IB IRA account. Only
					required if the destination account at IB is an IRA account.
					The type must match the IB account type otherwise transfer
					will be rejected.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="margin_loan" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if customer has a margin loan at
					third-party broker.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="short_pos" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if customer's account at the third-party
					holds any short positions.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="option_pos" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if customer's account at the third-party
					holds any options positions.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ib_acct" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Customer Account at IB to which to transfer positions. Only
					relevant for independent position transfer loads.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="third_party_type" type="xsd:string" use="optional" default="Not informed">
			<xsd:annotation>
				<xsd:documentation>
					Third party type for ACATS transfers
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="approximate_acct_value" type="xsd:positiveInteger" use="optional" default="1">
			<xsd:annotation>
				<xsd:documentation>
					Approximate account values for ACATS transfers
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="DepositNotificationType">
		<xsd:annotation>
			<xsd:documentation>
				Notification details for deposit to be used to fund the account
				from an external source.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CheckDetails" type="CheckDetailsType"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="WireDetails" type="WireDetailsType" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="ACHDetails" type="ACHDetailsType" minOccurs="0"
                         maxOccurs="1" />
			<xsd:element name="IRADepositDetails" type="IRADepositDetailsType"
                         minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="type" type="Deposit_Type" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Method being used to deposit funds in the IB account from an
					external source.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="amount" type="xsd:decimal" use="required">
			<xsd:annotation>
				<xsd:documentation>
					The amount being deposited.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="Currency_Type" default="USD"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The currency in which amount is being deposited. For check
					deposits and customer initiated ACH transfers, the only
					supported currency is USD.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ib_acct" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Customer Account at IB for which this Deposit Notification
					is being set. Only relevant for notifications sent as part
					of Account Management Requests for open IB accounts.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FullyPaidSLBRequestType">
		<xsd:annotation>
			<xsd:documentation>
				Notification details for fully paid Stock Lend and Borrow
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="ib_acct" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Customer Account at IB for which this SLB request is being made.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="user_name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					User Name at IB for which this SLB request is being made.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="CheckDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Details of the check being used to fund the customer account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="check_number" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The Check Number of the check being used to fund the
					deposit.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="routing_number" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					ABA Routing Number of the Bank on which the check is drawn
					as noted on the check.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="acct_number" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Account Number of the customer at the Bank on which the
					check is drawn as noted on the check.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="WireDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Details of the wire being used to fund the customer account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="bank_name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Name of the Bank from which the deposit Wire Transfer will
					originate.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="reference_number" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Reference Number or Payment ID of the Wire Transfer provided
					by the Bank.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ACHDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Details of the ACH transfer being used to fund the customer
				account.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="cust_init_ACH" type="xsd:boolean" default="true"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the ACH deposit will be initiated by the
					customer or by IB. Current support limited to customer
					initiated, the default.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bank_name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Name of the Bank from which the ACH transfer will originate.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="IRADepositDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Additional deposit details relevant to a deposit to an IRA
				account. Required for deposit notifications for IRA accounts.
				These details must be provided in addition to the basic deposit
				type specific details.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="deposit_type" type="IRADeposit_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Whether a contribution or a rollover.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tax_year" type="IRAContribTaxYear_Type"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Tax year of the contribution for contrbution deposits. Prior
					year contributions only allowed until the tax deadline
					(4/15). Such deposits will only be allowed if they settle on
					time (by 4/15).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="from_ira_type" type="IRA_Type" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Type of the source IRA account, only required for rollover
					deposits. Rollover deposits currently restricted to be from
					Traditional, Roth and SEP IRA accounts only. Inherited IRA
					accounts may not be the source of a rollover deposit
					notification.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="IndividualTaxInformation">
		<xsd:annotation>
			<xsd:documentation>
				Tax information about the customer required by the IRS. A Form
				W-9 or W-8 is required for all customers except customers of
				IB-India.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="W9" type="FormW9" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required for a US citizen or US person including
						resident alien individual.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="W8Ben" type="FormW8BEN" minOccurs="0"
                         maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						Required if the beneficial owner is not a US person.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="FormW8BEN">
		<xsd:annotation>
			<xsd:documentation>
				Required information for completing a Form W-8BEN for the
				customer certifying Foreign Status of Beneficial Owner for US
				tax withholding. Address details will be defaulted from the
				application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Name of Individual or Organization that is the beneficial
					owner.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ben_type" type="W8Ben_Type" use="required" />
		<xsd:attribute name="tin" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					US TIN if required in Part I item 6. Must be numeric, 9
					digits no dashes.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tin_type" type="TIN_Type" use="optional">
			<xsd:annotation>
				<xsd:documentation>Type of TIN provided.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="foreign_tax_id" type="xsd:string" use="optional" />
		<xsd:attribute name="reference_number" type="xsd:string"
                       use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9a" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if certifying item 9a in Part II Claim of
					Tax Treaty Benefits.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9a_country" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Treaty Country, required if certifying item 9a in Part II
					Claim of Tax Treaty Benefits.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9b" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if certifying item 9b in Part II Claim of
					Tax Treaty Benefits.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9c" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if certifying item 9c in Part II Claim of
					Tax Treaty Benefits.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9d" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if certifying item 9d in Part II Claim of
					Tax Treaty Benefits.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_9e" type="xsd:boolean" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if certifying item 9e in Part II Claim of
					Tax Treaty Benefits. Setting this to true currently will
					result in the application being rejected.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_10_article" type="xsd:string"
                       use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_10_percent" type="xsd:string"
                       use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_10_income_type" type="xsd:string"
                       use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_2_10_reasons" type="xsd:string"
                       use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="part_3_11" type="xsd:boolean" use="prohibited">
			<xsd:annotation>
				<xsd:documentation>Currently not supported.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="FormW9">
		<xsd:annotation>
			<xsd:documentation>
				Required information for completing a Form W-9 Request for TIN
				and Certification. Address details will be defaulted from the
				application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					As shown on the Income Tax Return.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="business_name" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					If different from name on tax return.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="customer_type" type="W9Customer_Type"
                       use="required" />
		<xsd:attribute name="tax_classification" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required only if customer type is LLC. Can be one of
					D=disregarded entity, C=corporation, P=partnership or free
					form text.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="other_customer_type" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Required details if customer type is Other.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tin" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					The Tax Identification Number specified in Part I. Must be
					numeric, 9 digits no dashes.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="tin_type" type="TIN_Type" use="optional"
                       default="SSN">
			<xsd:annotation>
				<xsd:documentation>
					Type of TIN provided, defaulted to SSN.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cert1" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if agreeing to certification 1 in part II.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cert2" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if agreeing to certification 2 in part II.
					Must be set to false if notified by IRS as being subject to
					backup withholding.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="cert3" type="xsd:boolean" use="required">
			<xsd:annotation>
				<xsd:documentation>
					To be set to True if agreeing to certification 3 in part II.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:element name="AcctMgmtRequests">
		<xsd:annotation>
			<xsd:documentation>
				A set of account management function requests for accounts
				belonging to a specific external entity. Account specified on
				any request must already be open.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="DuplicateAcct" type="DuplicateAcctRequest"
                             minOccurs="0" maxOccurs="unbounded" />
				<xsd:element name="DepositNotification"
                             type="DepositNotificationType" minOccurs="0" maxOccurs="unbounded" />
				<xsd:element name="FullyPaidSLBRequests"
                             type="FullyPaidSLBRequestType" minOccurs="0" maxOccurs="unbounded" />
				<xsd:element name="RecurringTransaction"
                             type="RecurringTransactionType" minOccurs="0" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="DuplicateAcctRequest">
		<xsd:annotation>
			<xsd:documentation>
				Request to duplicate customer's existing account at IB. Only
				available for Advisor Client accounts. In addition to trading
				configuration, Advisor Client Fees and Commission Schedules will
				also be duplicated. the duplicate account will have a different
				account id and will be accessible with the customer's existing
				user session.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="reference_account_id" type="xsd:string"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					IB Account ID of the Advisor Client account to be
					duplicated.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="no_of_duplicates" type="xsd:nonNegativeInteger"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Number of duplicate accounts required, if not provided
					defaults to one.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="Agreements">
		<xsd:annotation>
			<xsd:documentation>
				List of agreements that are a part of the IB Customer
				Application process that were executed by the customer on the
				remote application which generated this Electronic Customer
				Application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Agreement" type="Agreement"
                         maxOccurs="unbounded" />
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Agreement">
		<xsd:annotation>
			<xsd:documentation>
				Details of execution of an agreement that is a part of the IB
				Customer Application process that was executed by the customer
				on the remote application which generated this Electronic
				Customer Application.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="form_no" type="xsd:nonNegativeInteger"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					IB Form Number for the Agreement.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="exec_login_ts" type="xsd:nonNegativeInteger"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Login timestamp for the session in which the customer
					executed this agreement on the remote application in the ISO
					8601 Basic Format YYYYMMDDhhmmss.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="exec_ts" type="xsd:nonNegativeInteger"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Timestamp of the execution of the agreement by the customer
					on the remote application in the ISO 8601 Basic Format
					YYYYMMDDhhmmss. Execution here refers to the customer
					hitting enter after acknowledging or signing the agreement
					depending on whether the agreement requires just an
					acknowledgement or a signature.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="signature" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Character string typed in by the customer as signature to
					execute an agreement requiring a signature. Must match the
					name of the account signatory.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_acct_id" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Identifier for the account at the external entity for which
					the customer executed this agreement. This id will be used
					to record the agreement for the corresponding IB account.
					Only required for agreements needed per account for a
					customer opening multiple accounts. For single accounts this
					is implicit.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="external_individual_id" type="xsd:string"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Identifier at the external entity for the individual
					executing the agreement. Must be an individual listed on the
					application. Ignored for INDIVIDUAL applications as
					agreements must be executed by the Account Holder.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="RecurringTransactionType">
		<xsd:annotation>
			<xsd:documentation>
				Details of scheduled periodic withdrawals or deposits between
				the account and an external source. Currently, only ACH
				withdrawals are supported.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ACHInstruction" type="ACHInstructionType"
                         minOccurs="0" maxOccurs="1" />
			<xsd:element name="IRAWithdrawalDetails"
                         type="IRAWithdrawalDetailsType" minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="type" type="RecurringTx_Type" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Type of recurring transaction, Deposit/withdrawal.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="method" type="Deposit_Type" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Method being used to transfer funds between the IB account
					and external source.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="instruction" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Name of the instruction used to transfer funds. May be the
					name of an existing instruction for the account or a new
					instruction being specified as part of the transaction
					request.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="frequency" type="RecurringTxFreq_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					Frequency at which the transfer should occur.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="start_date" type="xsd:date" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Date from which transfers should commence.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="end_date" type="xsd:date" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Date on which transfers should terminate. When not
					specified, will be repeated perpetually.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Nick name for the recurring transaction instruction.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="amount" type="xsd:decimal" use="required">
			<xsd:annotation>
				<xsd:documentation>
					The amount being deposited.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="Currency_Type" default="USD"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The currency in which amount is being deposited/withdrawn.
					For check deposits, customer initiated ACH transfers and IB
					initiated ACH transfers, the only supported currency is USD.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ib_acct" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Customer Account at IB for which this recurring transaction
					is being set. Only relevant for instructions sent as part of
					Account Management Requests for open IB accounts.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ACHInstructionType">
		<xsd:annotation>
			<xsd:documentation>
				Details of an ACH instruction to be used to transfer funds
				between the account and an external source. Currently only
				instructions for credit transactions initiated by IB are
				supported.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="cust_init_ACH" type="xsd:boolean" default="false"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Flag to indicate if the ACH deposit will be initiated by the
					customer or by IB. Current support limited to IB initiated,
					the default.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="type" type="ACHInstructionTx_Type" default="CREDIT"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Type of transactions for which this instruction may be used.
					Currently, only credit is supported.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Nick name for the ACH instruction.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="bank_country" type="CountryCode_Type" default="USA"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The bank country, currently, ACH is only supported for US
					banks.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="currency" type="Currency_Type" default="USD"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Currency of the bank account, currently, only USD bank
					accounts are supported.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="routing_number" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					ABA Routing Number of the Bank.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="acct_number" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Account Number of the customer at the Bank.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="IRAWithdrawalDetailsType">
		<xsd:annotation>
			<xsd:documentation>
				Additional details relevant to a withdrawal from an IRA account.
				Required for withdrawal instruction for IRA accounts. These
				details must be provided in addition to the basic withdrawal
				type specific details.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="distribution_type" type="IRADistribution_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>Type of distribution.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="excess_contrib_yr" type="xsd:nonNegativeInteger"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					Year from which excess contribution is being withdrawn.
					Required if the distribution type specified is
					EXCESS_CONTRIB.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="fed_tax_rate" type="xsd:decimal" use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The tax rate for federal withholding from the withdrawal. If
					not specified, implies customer indicates no withholding.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="legal_residence_state" type="StateCode_Type"
                       use="required">
			<xsd:annotation>
				<xsd:documentation>
					State of legal residence of the customer.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="state_tax_rate" type="xsd:decimal"
                       use="optional">
			<xsd:annotation>
				<xsd:documentation>
					The tax rate for state withholding from the withdrawal. If
					not specified, implies customer indicates no withholding.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SimpleLegalEntityType">
		<xsd:annotation>
			<xsd:documentation>
				A legal entity is anything other than an individual. A simple
				legal entity being asscoiated with an account in tertiary
				capacity such as IRA beneficiary where the entity's name is the
				only required piece of information.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="EntityName" />
			<xsd:element name="Location" type="Address" minOccurs="0"
                         maxOccurs="1" />
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="optional" />
		<xsd:attribute name="external_id" type="xsd:string" use="required" />
	</xsd:complexType>

   <!--
    ** Types used on Account Support info
    ** Organizations must be one of Operating Business, Family Vehicle, Proprietary Trading or Financial Institutition
    ** Depending of type, different additional info is required.
   -->
   <xsd:complexType name="PrimaryContributorType">
		<xsd:annotation>
			<xsd:documentation>
				Family vehicle accts must have a primary contributor
			</xsd:documentation>
		</xsd:annotation>
            <xsd:sequence>
			<xsd:element name="FirstName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="MiddleInitial" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="LastName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Suffix" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Employer" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Occupation" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Address" type="Address" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="SourceOfFunds" type="xsd:string" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AdministratorType">
		<xsd:annotation>
			<xsd:documentation>
				Family vehicle accts must have a primary contributor
			</xsd:documentation>
		</xsd:annotation>
            <xsd:sequence>
			<xsd:element name="FirstName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="MiddleInitial" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="LastName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Suffix" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Address" type="Address" minOccurs="1" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AdministratorContactPersonType">
		<xsd:annotation>
			<xsd:documentation>
				Family vehicle accts must have a primary contributor
			</xsd:documentation>
		</xsd:annotation>
            <xsd:sequence>
			<xsd:element name="FirstName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="MiddleInitial" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="LastName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="Suffix" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="PhoneNumber" type="xsd:string" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AccountSupportType">
		<xsd:annotation>
			<xsd:documentation>
				Account Support classifies organizations in 4 categories of
                        trading intentions.
			</xsd:documentation>
		</xsd:annotation>
            <xsd:sequence>
			<xsd:element name="BusinessDescription" type="xsd:string" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="PrimaryContributor" type="PrimaryContributorType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Administrator" type="AdministratorType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="AdministratorContactPerson" type="AdministratorContactPersonType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="OwnersResideUS" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="SolicitOwnersResideUS" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="AcceptOwnersResideUS" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
		<xsd:attribute name="type" type="TradeIntention_Type" use="required" />
	</xsd:complexType>

</xsd:schema>
