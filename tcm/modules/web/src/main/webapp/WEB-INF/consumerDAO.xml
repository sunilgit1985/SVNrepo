<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="manageAccountDAO" class="com.invessence.web.dao.ManageAccountDAO">
   		<property name="dataSource" ref="dataSource"/>
   	</bean>

    <bean id="positionDAO" class="com.invessence.web.dao.common.PositionDAO">
   		<property name="dataSource" ref="dataSource"/>
   	</bean>

    <bean id="consumerListDataDAO" class="com.invessence.web.dao.consumer.ConsumerListDataDAO" >
   		<property name="dataSource" ref="dataSource"/>
   	</bean>

    <bean id="consumerSaveDataDAO" class="com.invessence.web.dao.consumer.ConsumerSaveDataDAO" >
   		<property name="dataSource" ref="dataSource"/>
   	</bean>

    <bean id="aggregationDAO" class="com.invessence.web.dao.consumer.AggregationDAO" >
   		<property name="dataSource" ref="dataSource"/>
   	</bean>


</beans>
